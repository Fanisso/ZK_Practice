	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<display-name>ReplaceAuRequestWithWebSocket</display-name>
	<servlet>
		<servlet-name>testWebSocketServlet</servlet-name>
